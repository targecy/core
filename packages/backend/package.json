{
  "name": "@targecy/backend",
  "version": "0.0.1",
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "trpc-api-export": "tsup --config trpc-api-export/builder/tsup.config.ts && npm run format-fix",
    "prepare": "husky install",
    "build": "yarn graphql-codegen && shx rm -rf build && tsc -p tsconfig.json",
    "dev": "yarn graphql-codegen && concurrently \"ts-node-dev -r tsconfig-paths/register src/index.ts\" \"yarn graphql-codegen-watch\"",
    "start": "NODE_PATH=./build/src node build/src/index.js",
    "lint": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore ./src",
    "lint:fix": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore --fix ./src ",
    "lint-staged-husky": "lint-staged",
    "graphql-codegen-watch": "gql-gen --config codegen.ts  --watch",
    "graphql-codegen": "gql-gen --config codegen.ts",
    "tsc": "tsc -p tsconfig.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev --name init",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:seed": "prisma db seed",
    "format-lint": "prettier --config .prettierrc --check --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown -l .",
    "semantic-release": "semantic-release --branches main"
  },
  "dependencies": {
    "@0xpolygonid/js-sdk": "^1.0.3",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript": "3.0.4",
    "@graphql-codegen/typescript-graphql-request": "6.0.1",
    "@graphql-codegen/typescript-operations": "3.0.4",
    "@iden3/js-crypto": "^1.0.0",
    "@iden3/js-iden3-core": "^1.0.1",
    "@prisma/client": "4.9.0",
    "@trpc/server": "10.40.0",
    "cors": "2.8.5",
    "express": "4.18.2",
    "graphql": "^16.8.0",
    "graphql-codegen-typescript-common": "0.18.2",
    "graphql-request": "6.1.0",
    "graphql-tag": "2.12.6",
    "polygonscan-api": "^1.0.4",
    "superjson": "1.13.1",
    "viem": "^1.11.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@types/cors": "2.8.13",
    "prisma": "4.9.0",
    "@types/express": "4.17.17",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "concurrently": "7.2.1",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "path": "^0.12.7",
    "prettier": "3.0.2",
    "semantic-release": "21.1.0",
    "shx": "0.3.4",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "tsup": "7.2.0",
    "typescript": "5.2.2"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "engines": {
    "node": ">=18.17.1"
  }
}
