{
  "name": "@targecy/relayer",
  "version": "0.0.4",
  "scripts": {
    "build": "shx rm -rf build && tsc -p tsconfig.json && yarn export-types",
    "dev": "concurrently \"ts-node-dev -r tsconfig-paths/register src/index.ts\" \"nodemon\"",
    "export-types": "rm -rf types && rm -rf ../sdk/src/generated/types/relayer && npx tsc --declaration --outDir types && cp -r types/src/trpc ../sdk/src/generated/types/relayer && rm -rf types",
    "start": "node build/src/index.js",
    "lint": "eslint --max-warnings 0 .",
    "lint-fix": "eslint --fix .",
    "lint-staged-husky": "lint-staged",
    "tsc": "tsc -p tsconfig.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev --name init",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:seed": "prisma db seed",
    "format-lint": "prettier --config .prettierrc --check --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown -l .",
    "semantic-release": "semantic-release --branches main"
  },
  "dependencies": {
    "@prisma/client": "4.9.0",
    "@trpc/server": "10.40.0",
    "cors": "2.8.5",
    "ethers": "^6.9.0",
    "express": "4.18.2",
    "os": "^0.1.2",
    "superjson": "1.13.1",
    "typescript": "5.2.2",
    "viem": "^1.11.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "concurrently": "^8.2.2",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "nodemon": "^3.0.2",
    "prettier": "3.0.2",
    "prisma": "4.9.0",
    "semantic-release": "21.1.0",
    "shx": "0.3.4",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "tsup": "7.2.0"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "engines": {
    "node": "18"
  }
}
