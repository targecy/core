{
  "name": "@scaffold-eth/solidity",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "chain:hardhat": "cross-env BUILDING=true hardhat node --network hardhat",
    "test:hardhat": "yarn hardhat coverage --network hardhat",
    "test:watch:hardhat": "yarn hardhat watch test --network hardhat",
    "compile": "tsc --project tsconfig.json",
    "compile:hardhat": "cross-env BUILDING=true hardhat compile",
    "compile:watch:hardhat": "cross-env BUILDING=true hardhat watch compile",
    "contracts:build": "yarn workspace @scaffold-eth/common contracts:build",
    "watch:hardhat": "cross-env BUILDING=true yarn hardhat watch compile",
    "deploy": "hardhat deploy --export-all ../common/src/generated/hardhat_contracts.json",
    "deploy:mumbai": "hardhat deploy --network mumbai --export-all ../common/src/generated/hardhat_contracts.json",
    "deploy:matic": "hardhat deploy --network matic --export-all ../common/src/generated/hardhat_contracts.json",
    "upgrade": "hardhat upgrade",
    "propose:upgrade:mumbai": "hardhat run --network mumbai scripts/propose.upgrade.js",
    "propose:upgrade:matic": "hardhat run --network matic scripts/propose.upgrade.js",
    "deploy:post": "yarn ts-node scripts/hardhatPostDeploy.ts",
    "seed": "yarn ts-node scripts/seed.ts",
    "seed:force": "yarn ts-node scripts/seed.ts localhost force",
    "seed:mumbai:force": "yarn ts-node scripts/seed.ts mumbai force",
    "clean": "yarn cross-env BUILDING=true hardhat clean && rm -rf ./generated/*",
    "clean:deployments": "rm -rf ./deployments/* && rm -rf ./generated/* && rm -rf ./.openzeppelin/*",
    "accounts:hardhat": "hardhat accounts",
    "generate": "hardhat generate",
    "account": "hardhat account",
    "lint": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore ./scripts/deploy/**/*.ts ./tasks/**/*.ts ./tests/**/*.ts ./helpers/**/*.ts",
    "solhint": "solhint ./contracts/**/*.sol",
    "format": "yarn prettier -w ./scripts/deploy/**/*.ts ./tasks/**/*.ts ./tests/**/*.ts ./helpers/**/*.ts contracts/**/*.sol ",
    "format:check": "yarn prettier -c ./scripts/deploy/**/*.ts ./tasks/**/*.ts ./tests/**/*.ts ./helpers/**/*.ts ./contracts/**/*.sol "
  },
  "dependencies": {
    "@atixlabs/hardhat-time-n-mine": "^0.0.5",
