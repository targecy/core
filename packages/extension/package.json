    "postcss-loader": "^8.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2 ",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.0",
    "ts-loader": "^8.0.0",
    "typescript": "^5.3.3",
