{
  "name": "@targecy/webapp",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "yarn set:config && yarn graphql-codegen && next dev -p 3090",
    "dev": "yarn set:config && yarn graphql-codegen && concurrently \"next dev -p 3090\" \"yarn graphql-codegen-watch\"",
    "build": "yarn set:config && tsc --noEmit && yarn graphql-codegen && yarn build:anttheme && next build",
    "serve": "next start",
    "lint:next": "next lint",
    "lint": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore ./src",
    "lint:fix": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore --fix ./src ",
    "build:anttheme": "yarn lessc -x -js ./src/styles/themes/ant-light-theme.less ./public/ant-light-theme.css && yarn lessc -x -js ./src/styles/themes/ant-dark-theme.less ./public/ant-dark-theme.css",
    "contracts:hardhat": "yarn shx rm -rf './src/generated/contracts-types' && yarn shx cp -r '../solidity-ts/generated/contract-types' './src/generated'",
    "contracts:external": "yarn shx rm -rf './src/generated/external-contracts' && yarn shx cp -r './scripts/eth-sdk-config.ts.bak' './src/generated/eth-sdk.config.ts' && yarn eth-sdk -p ./src/generated",
    "format": "yarn prettier -w ./src ",
    "format:check": "yarn prettier -c ./src ",
    "dev:link:components": "yalc link eth-components",
    "dev:link:hooks": "yalc link eth-hooks",
    "dev:unlink": "yalc remove eth-hooks && yalc remove eth-components",
    "graphql-codegen-watch": "gql-gen --config codegen.ts  --watch",
    "graphql-codegen": "gql-gen --config codegen.ts",
    "set:config": "yarn generate:config:hostname",
    "generate:config:hostname": "./setHostname.sh"
  },
