{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.cache": true,
    "**/.vs/": true,
    "**/*.cs": true,
    "**/*.orig": true,
    "**/bin/": true,
    "**/build/": true,
    "**/build/**/*": true,
    "**/debug/": true,
    "**/dist/": true,
    "**/obj": true,
    "yarn-error.log": true,
    "**/yarn-error.log": true,
    "**/.yalc": true,
    "**/node_modules": false
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "search.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.cache": true,
    "**/.vs/": true,
    "**/*.cs": true,
    "**/*.orig": true,
    "**/bin/": true,
    "**/build/": true,
    "**/build/**/*": true,
    "**/debug/": true,
    "**/dist/": true,
    "**/obj": true,
    "node_modules": true,
    "node_modules/*/**": true,
    "**/node_modules": true,
    "yarn-error.log": true,
    "**/yarn-error.log": true,
    "**/.yalc": true,
    "**/.yarn": true,
    "yarn.lock": true
  },
  "explorerExclude.backup": null,
  "eslint.debug": true,
  "eslint.enable": true,
  "eslint.workingDirectories": [
    { "directory": "packages/*", "changeProcessCWD": true },
    { "directory": "packages/solidity-ts-ts", "changeProcessCWD": true },
    { "directory": "packages/webapp-ts", "changeProcessCWD": true },
    { "directory": "packages/common", "changeProcessCWD": true },
    { "directory": "packages/subgraph", "changeProcessCWD": true },
    { "directory": "packages/services", "changeProcessCWD": true },
    { "directory": "packages/commands", "changeProcessCWD": true }
  ],
  "solidity.packageDefaultDependenciesDirectory": "packages/solidity-ts-ts/node_modules",
  "editor.formatOnSave": true,
  "solidity.formatter": "prettier", // This is the default so it might be missing.
  "[solidity]": {
    "editor.defaultFormatter": "NomicFoundation.hardhat-solidity"
  },
  "solidity.defaultCompiler": "localNodeModule",
  "mochaExplorer.configFile": "./packages/solidity-ts-ts/.mocharc.json",
  "cSpell.words": [
    "codegen",
    "datasource",
    "Iden",
    "ipfs",
    "Merkle",
    "rainbowkit",
    "targecy",
    "trpc",
    "viem",
    "Wagmi",
    "zkprequest",
    "zkprequests"
  ]
}
